<IfModule mod_rewrite.c>
  Options -Multiviews
  RewriteEngine On
  RewriteBase /gamecompany.azurewebsites.net/public
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{HTTP_HOST} ^gamecompany\.azurewebsites\.net$ [NC]
  RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule>